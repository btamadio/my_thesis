@article{jet-algo-review,
  author={Ryan Atkin},
  title={Review of jet reconstruction algorithms},
  journal={Journal of Physics: Conference Series},
  volume={645},
  number={1},
  pages={012008},
  url={http://stacks.iop.org/1742-6596/645/i=1/a=012008},
  year={2015},
  abstract={Accurate jet reconstruction is necessary for understanding the link between the unobserved partons and the jets of observed collimated colourless particles the partons hadronise into. Understanding this link sheds light on the properties of these partons. A review of various common jet algorithms is presented, namely the K t , Anti- K t , Cambridge/Aachen, Iterative cones and the SIScone, highlighting their strengths and weaknesses. If one is interested in studying jets, the Anti- K t algorithm is the best choice, however if ones interest is in the jet substructures then the Cambridge/Aachen algorithm would be the best option.}
}

@Article{jet-jetography,
author="Salam, Gavin P.",
title="Towards jetography",
journal="The European Physical Journal C",
year="2010",
month="Jun",
day="01",
volume="67",
number="3",
pages="637--686",
abstract="As the LHC prepares to start taking data, this review is intended to provide a QCD theorist's understanding and views on jet finding at hadron colliders, including recent developments. My hope is that it will serve both as a primer for the newcomer to jets and as a quick reference for those with some experience of the subject. It is devoted to the questions of how one defines jets, how jets relate to partons, and to the emerging subject of how best to use jets at the LHC.",
issn="1434-6052",
doi="10.1140/epjc/s10052-010-1314-6",
url="https://doi.org/10.1140/epjc/s10052-010-1314-6"
}

